@using FADMS.DAL.AuthService.Interfaces
@using System

@inject IUserInfoes userInfo
@{

    var userInfos = await userInfo.GetUserInfoByUser(@User.Identity.Name);
    var userthana = await userInfo.GetUserInfoByThana(@User.Identity.Name);
    var menu = await userInfo.GetMenuByUserName(User.Identity.Name);
    var InfoImage = await userInfo.GetUserInfoByUserNameNew(@User.Identity?.Name);
    //string orgTitle = string.Empty;


    string url = "";
    string admin = "অ্যাডমিন ইউজার";
    string Dealeradmin = "ডিলার অ্যাডমিন ইউজার";
    string Dealer = "ডিলার ইউজার";
    string thana = "থানা ইউজার";
    string dist = "জেলা অ্যাডমিন ইউজার";
    string distGen = "জেলা জেনারেল ইউজার";
    var userRoleId = await userInfo.GetUserRoleIdByUserName(User.Identity.Name);
    //Dealer & admin
    if (userRoleId == "9ae09ccf-6fd3-4b36-aabb-95324e13485c")
    {
        url = Dealeradmin;
    }
    //Dealer & admin
    if (userRoleId == "5774a0e1-f858-4d82-a29a-5b9b0e003afc")
    {
        url = Dealer;
    }
    //sbadmin & admin
    if (userRoleId == "a3b6f86f-f910-4387-b579-041e4b8a1792" || userRoleId == "04AED4C4-EF8F-4704-9EC5-0B38CAAACBEB")
    {
        url = admin;
    }
    //DistrictSbAdmin
    if (userRoleId == "809f6aac-93dc-4407-a1f2-93be4f8f01f0")
    {
        url = @userthana?.district?.districtNameBn + "  " + dist;
    }
    //District General User
    if (userRoleId == "8211f581-a71e-4fc9-b5df-12b84d6aeec9")
    {
        url = @userthana?.district?.districtNameBn + "  " + distGen;
    }
    //Thana user
    if (userRoleId == "95f288f0-a648-445a-8eb9-7c734257fab5")
    {
        url = @userthana?.thana?.thanaNameBn + "  " + thana;
    }

}
