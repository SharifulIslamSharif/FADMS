                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, Update it!'
            }).then(function () {
                $.ajax({
                    url: '@Url.Action("Photo", "PersonalInfo", new { area= "FAMSAPP" })',
                    data: formData,
                    type: 'POST',
                    dataType: 'json',
                    contentType: false,
