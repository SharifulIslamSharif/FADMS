@{
    var total = Model.TotalLicense.ToString();
    var totalPolitical = Model.politicalLilist.ToString();
    var nmb = String.Empty;
    for (int i = 0; i < total.Length; i++)
    {
        var x = total[i];
        var eng = "1234567890";
        var bn = "১২৩৪৫৬৭৮৯০";
        var bng = bn[eng.IndexOf(x)];
        nmb += bng;
    }
}

<div class="row mt-2">
    <div class="card-header col-md-6" style="background-color:#F7F7F7;">
        <div class="border-left-primary h-100 py-2 font-weight-bold text-center divhov" onclick="TotalLi()"
             style="background-color:antiquewhite;  height: 100%; border-radius: 6px; width: 43%; border:2px solid double;
            font-size:large; margin-left: 2%;">
            <i class="fab fa-airbnb"></i>&nbsp;&nbsp; মোট লাইসেন্স : <span id="totalLi"> @nmb</span>
        </div>
    </div>
    <div class="card-header col-md-6 row" style="background-color:#F7F7F7;">
        <div class=" col-md-6 row">
            <input type="text" id="allSearch" class="form-control" placeholder="লাইসেন্স নম্বর দ্বারা অনুসন্ধান করুন"
                   style="border-radius: 25px; border-color:#18c1ef; height:46px; width: 88%;  position:absolute" />
            <button class="btn btn-info" id="btnSearch" onclick="Search()" title="Search"><i class="fas fa-search"></i></button>
        </div>
        @*<div class=" col-md-2"></div>*@
        <div class=" col-md-6" style="margin-left: 20px;">
            <button class="btn btn-info float-right" style="height:45px;" id="btnAdvanced" onclick="AdvancedSrc()">অ্যাডভান্স অনুসন্ধান</button>
        </div>

    </div>
</div>

<div class="row mt-2 mr-4 ml-1">
    @foreach (var data in Model.armsTypeInfo)
    {
        var total1 = data.Total.ToString();
        var nmb1 = String.Empty;
        for (int i = 0; i < total1.Length; i++)
        {
            var x = total1[i];
            var eng = "1234567890";
            var bn = "১২৩৪৫৬৭৮৯০";
            var bng = bn[eng.IndexOf(x)];
            nmb1 += bng;
        }
        <div class="col-md-4 mb-4" onclick="LiDetailsByLiType(@data.Id)">
            <div class="card border-left-primary shadow h-100 py-2 divhov">
                <div class="card-body">
                    <div class="row no-gutters align-items-center">
                        <div class="col-10">
                            <div class="text-xl-left font-weight-bold text-primary mb-1">@data.TypeName</div>
                            <div class="h5 mb-0 font-weight-bold text-gray-800">
