        function GetTotalApplicantDashBoardMapDivision() {
            $.ajax({
                contentType: "application/json; charset=utf-8",
                url: '@Url.Action("GetTotalApplicantDashBoardMapDivision", "Home", "http")',
                type: "GET",
                dataType: "json",
                async: true,
                data: {},
                success: function (response) {
                    var res = response
                    $.each(res, function (id, option) {

                        var total = option.Total.toString();
                        var nmb = '';
                        for (var i = 0; i < total.length; i++)
                        {
                            var x = total[i];
                            var eng = "1234567890";
                            var bn = "১২৩৪৫৬৭৮৯০";
                            var bng = bn[eng.indexOf(x)];
                            nmb += bng;
                        }

                        if (option.TypeName == "খুলনা") {
                            //$("#divKhulnaNo").html(option.Total);
                            $("#divKhulnaNo").html(nmb);
                            $("#divKhulnaID").val(option.DId);
                        } else if (option.TypeName == "চট্টগ্রাম") {
                            //$("#divChittagongNo").html(option.Total);
                            $("#divChittagongNo").html(nmb);
                            $("#divChittagongID").val(option.DId);
                        } else if (option.TypeName == "ঢাকা") {
