        function Search() {
            var liNumber = $('#allSearch').val();
            if (liNumber == "") {
                swal('Warning!', 'Please insert License Number', 'warning');
                return false;
            }
            $.ajax({
                contentType: "application/json; charset=utf-8",
                url: '@Url.Action("GetLicenseByLicenseNumber", "Home", "http")',
                type: "GET",
                dataType: "json",
                async: true,
                data: { liNo: liNumber },
                success: function (response) {
                    console.log(response);
                    if (response == 'noLiNO') {
                        swal('Warning!', 'Please insert Valid License Number', 'warning');
                        return false;
                    }
                    $("#searchModal").modal("show");
                    var tbl = $('#tblSearch');
                    var tbody = tbl.find('tbody');
                    tbody.empty();
                    var url = '';

                    if (response.LicenseTypeId == 1) {
                        url = '/FAMSAPP/PersonalInfo/Menu?id=' + response.LiIdFor
                    }
                    else if (response.LicenseTypeId == 2) {
                        url = '/FAMSDB/FinancialOrganization/menu/' + response.LiIdFor
                    }
                    else if (response.LicenseTypeId == 3) {
                        url = '/FAMSDB/FinancialOrganization/menu/' + response.LiIdFor
                    }
                    var trHtml = '<tr><td><img style="width: 36%;" src="' + response.ImgUrl + '" /></td></tr>' +
                        '<tr><td><span style="font-size:14pt; color:black; font:bold; color:black;">লাইসেন্স নং</span> : <span style="color:cornflowerblue">' + response.l + '</span></td></tr>' +
                        '<tr><td><span style="font-size:14pt; color:black; font:bold; color:black;">নাম</span> : <span style="color:cornflowerblue">' + response.LiName + '</span></td></tr>'+
                        '<tr><td><span style="font-size:14pt; color:black; font:bold; color:black;">লাইসেন্সের ধরন</span> : <span style="color:cornflowerblue">' + response.LicenseTypeName + '</span></td></tr>'+
                        '<tr><td><span style="font-size:14pt; color:black; font:bold; color:black;">আগ্নেয়াস্ত্রের ধরন</span> : <span style="color:cornflowerblue">' + response.ArmTypeName + '</span></td></tr>'+
                        '<tr><td> <a class="btn btn-info" style="height: 40px; width: 45%; font-size: 15pt !important;" href="' + url +'">বিস্তারিত দেখুন</a></td></tr>';
                    tbody.append(trHtml);
                    //window.location.href = url;
                },
                failure: function () {
                    $.alert.open("error", "Failed!");
                }
            });
        }
