<style>
    .small {
        font-size: 100%;
        font-weight: 800;
    }

    .bigMap-btn {
        width: 70px;
        height: 30px;
    }

    .divMap-btn {
        width: 70px;
        height: 30px;
    }

    .divhov {
        transition: transform .2s;
    }

        .divhov:hover {
            background-color: #9cf0f6;
            transform: scale(1.1);
            box-shadow: 5px 10px #888888
        }

