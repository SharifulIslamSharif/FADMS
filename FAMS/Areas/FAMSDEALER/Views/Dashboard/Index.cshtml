        }
        function pistolList(response) {
            $("#tblLiType").DataTable().destroy();
            var tbl = $("#tblLiType");
            var tbody = tbl.find('tbody');
            tbody.empty();
            var trHtml = '';
            $.each(response.dashboardArmsTypeViewModels, function (key, value) {

                var url = '';
