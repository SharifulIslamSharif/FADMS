        function purchaseItemsList(response) {
            $("#purchasetable").DataTable().destroy();
            var tbl = $("#purchasetable");
            var tbody = tbl.find('tbody');
            tbody.empty();
            var trHtml = '';
            $.each(response.dashboardPurchaseViewModels, function (key, value) {
                console.log(value.dealerName);
                var url = '';
                if (value.LicenseTypeId === 1) {
                    url = '/FAMSAPP/PersonalInfo/Menu?id=' + value.LiIdFor
                }

                trHtml += '<tr><td>' + value.dealerName + '</td><td>' + value.supplierName + '</td><td>' + value.armsType + '</td><td>' + value.poNo + '</td>' +
                    '<td>' + value.lcDate + '</td>' +
                    '<td>' + value.subTotal + '</td>' +
                    '<td><a class="btn btn-info" title="Edit" href="' + url + '"><i class="fa fa-edit"></i></a></td></tr>'
            })
            tbody.append(trHtml);
            $("#purchasetable").DataTable({
                stateSave: true,
                dom: "<'row'<'col-sm-12 col-md-2'l><'col-sm-12 col-md-3'B><'col-sm-12 col-md-7'f>>" +
                    "<'row'<'col-sm-12't>>" + "<'row'<'col-sm-12 col-md-6'i><'col-sm-12 col-md-6'pr>>",
