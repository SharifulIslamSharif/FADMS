@model FADMS.Areas.FAMSDEALER.Models.DashboardViewModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";

    #region Salse
    var pistol = Model.spSalesViewmodels.Where(x => x.ArmTypeName == "Pistol").FirstOrDefault();
    var pistolCount = (pistol?.Count >= 0) ? pistol?.Count : 0;

    var rivolber = Model.spSalesViewmodels.Where(x => x.ArmTypeName == "Revolver").FirstOrDefault();
    var rivolberCount = (rivolber?.Count >= 0) ? rivolber?.Count : 0;

    var oneTubeGun = Model.spSalesViewmodels.Where(x => x.ArmTypeName == "One Tube Gun").FirstOrDefault();
    var oneTubeGunCount = (oneTubeGun?.Count >= 0) ? oneTubeGun?.Count : 0;

    var twoTubeGun = Model.spSalesViewmodels.Where(x => x.ArmTypeName == "Two Tube Gun").FirstOrDefault();
    var twoTubeGunCount = (twoTubeGun?.Count >= 0) ? twoTubeGun?.Count : 0;

    var shotgun = Model.spSalesViewmodels.Where(x => x.ArmTypeName == "Shotgun").FirstOrDefault();
    var shotgunCount = (shotgun?.Count >= 0) ? shotgun?.Count : 0;

    var rifle = Model.spSalesViewmodels.Where(x => x.ArmTypeName == "Rifle").FirstOrDefault();
    var rifleCount = (rifle?.Count >= 0) ? rifle?.Count : 0;
    #endregion



    #region Purchase
    var pistol_p = Model.spPurchases.Where(x => x.ArmTypeName == "Pistol").FirstOrDefault();
    var pistol_pCount = (pistol_p?.Count >= 0) ? pistol_p?.Count : 0;

    var rivolber_p = Model.spPurchases.Where(x => x.ArmTypeName == "Revolver").FirstOrDefault();
    var rivolber_pCount = (rivolber_p?.Count >= 0) ? rivolber_p?.Count : 0;

    var oneTubeGun_p = Model.spPurchases.Where(x => x.ArmTypeName == "One Tube Gun").FirstOrDefault();
    var oneTubeGun_pCount = (oneTubeGun_p?.Count >= 0) ? oneTubeGun_p?.Count : 0;

    var twoTubeGun_p = Model.spPurchases.Where(x => x.ArmTypeName == "Two Tube Gun").FirstOrDefault();
    var twoTubeGun_pCount = (twoTubeGun_p?.Count >= 0) ? twoTubeGun_p?.Count : 0;

    var shotgun_p = Model.spPurchases.Where(x => x.ArmTypeName == "Shotgun").FirstOrDefault();
    var shotgun_pCount = (shotgun_p?.Count >= 0) ? shotgun_p?.Count : 0;

    var rifle_p = Model.spPurchases.Where(x => x.ArmTypeName == "Rifle").FirstOrDefault();
    var rifle_pCount = (rifle_p?.Count >= 0) ? rifle_p?.Count : 0;
    #endregion


}
@{
    var id = pistol?.Id;
    var revid = rivolber?.Id;
    var oneTubeGunId = oneTubeGun?.Id;
    var twoTubeGunId = twoTubeGun?.Id;
    var shotgunId = shotgun?.Id;
    var rifleId = rifle?.Id;

    var pistolid = pistol_p?.Id;
