                        <tbody>
                            @foreach (var data in Model.items)
                            {
                                <tr>
                                    <td>@data.itemCode</td>
                                    <td>@data.itemName</td>
                                    <td>@data.ArmType?.ArmTypeName</td>
                                    <td>@data.unit?.unitName</td>
                                    <td>@data.itemHsCode?.hsCode</td>
                                    <td>
                                        <a class="btn btn-success" title='Edit' onclick="Edit(@data.Id,'@data.itemName',@data.unitId,'@data.itemCode','@data.itemDescription',@data.parentId,'@data.itemHsCodeId','@data.itemHsCode?.Id','@data.costingMethod','@data.reorderQty','@data.productionTypeId','@data.itemHsCode?.CD','@data.itemHsCode?.TTI','@data.itemHsCode?.AIT','@data.itemHsCode?.VAT','@data.itemHsCode?.RD','@data.itemHsCode?.AT','@data.itemHsCode?.SD',@data.ArmTypeId)" href="#"><i class="fa fa-edit"></i></a>
                                        @*<a title="Delete" class="btn btn-danger" onclick="Remove(@data.Id)"><i class="fa fa-trash"></i></a>*@
                                    </td>
                                </tr>
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

