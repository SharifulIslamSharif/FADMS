                                <select class="form-control" name="armsTypeId" id="armsTypeId" data-parsley-required="true">
                                    <option value="">নির্বাচন করুন</option>
                                    @foreach (var item in Model.armTypes)
                                    {
                                        <option value="@item.Id">@item.ArmTypeName</option>
                                    }
                                </select>
                                <input type="hidden" id="parentId" name="parentId" value="0" />
                                @*<input type="hidden" id="parentName" name="parentName" value="" />*@

                                @*<input type="text" class="form-control" id="parentName" data-parsley-required="true">
                                    <input type="hidden" id="parentId" name="parentId" value="0" />*@


                            </div>
                            @*<div class="col-sm-1">
                                    <a href="#" title='Show Tree' class="btn btn-info" data-toggle="modal" onclick="ShowTree()" data-target="#addClusterModel" style="font-size:12px;">
                                        <span>
                                            <i class="fas fa-tree"></i>
                                        </span>
                                    </a>
                                </div>*@
                        </div>
                        <div class="form-group row">
                            <label for="addSpecification" class="col-sm-4 col-form-label">Add Specification</label>
                            <div class="col-sm-8">
                                <a href="#" title='Add Specification' class="btn btn-info" data-toggle="modal" data-target="#addItemSpecificationModal" style="font-size:12px;float:left;">
                                    <span>
                                        <i class="fas fa-plus-square"></i>
                                    </span>
                                </a>
                            </div>
                        </div>

                    </div>
                    <div class="col-md-6">
                        <div class="form-group row">
